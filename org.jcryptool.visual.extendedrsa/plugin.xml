<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
      		category="org.jcryptool.visual"
            class="org.jcryptool.visual.extendedrsa.ExtendedRSA_Visual"
            id="org.jcryptool.visual.extendedrsa.ExtendedRSAView"
            icon="icons/view.gif"
            name="Erweitertes RSA-Kryptosystem"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.jcryptool.core.operations.visuals">
      <Visual
            contextHelpId="org.jcryptool.visual.extendedrsa.rsaExtView"
            name="Erweitertes RSA-Kryptosystem"
            viewId="org.jcryptool.visual.extendedrsa.ExtendedRSAView">
      </Visual>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/toc.xml"
            primary="false">
      </toc>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>
</plugin>
