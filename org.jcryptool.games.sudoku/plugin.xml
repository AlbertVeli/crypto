<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="%view.name"
            icon="icons/view.gif"
            category="org.jcryptool.games"
            class="org.jcryptool.games.sudoku.views.SudokuView"
            id="org.jcryptool.games.sudoku.views.SudokuView">
      </view>
   </extension>
   <extension
         point="org.jcryptool.core.operations.games">
      <Game
            contextHelpId="org.jcryptool.games.sudoku.sudokuview"
            name="%Game.name"
            viewId="org.jcryptool.games.sudoku.views.SudokuView">
      </Game>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/toc.xml"
            primary="false">
      </toc>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>

</plugin>
